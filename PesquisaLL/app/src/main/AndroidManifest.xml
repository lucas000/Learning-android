<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pesquisaeletronica.com.br.pesquisall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DadosUsuarios"
            android:label="@string/titulo_formulario_usuario"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".DadosRepresentantes"
            android:label="@string/title_activity_dados_representantes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DadosPesquisa"
            android:label="@string/title_activity_dados_pesquisa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PesquisaEletronica"
            android:label="@string/title_activity_pesquisa_eletronica"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>